<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>




<VBox  fx:controller="projektTrzy.PrimaryController" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane fx:id="anchorPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="338.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
            <children>
                <Button layoutX="269.0" layoutY="65.0" mnemonicParsing="false" text="dodaj budzik" onAction="#addAlarm" />
                <ListView fx:id="listView" layoutX="40.0" layoutY="65.0" prefHeight="273.0" prefWidth="200.0" />
                <Button layoutX="269.0" layoutY="101.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" text="usun budzik" onAction="#removeAlarm" />
                <TextArea fx:id="hoursArea" editable="false" layoutX="295.0" layoutY="200.0" prefHeight="66.0" prefWidth="85.0" />
                <TextArea fx:id="minutesArea" editable="false" layoutX="397.0" layoutY="200.0" prefHeight="66.0" prefWidth="85.0" />
                <TextArea fx:id="secondsArea" editable="false" layoutX="497.0" layoutY="200.0" prefHeight="66.0" prefWidth="85.0" />
                <TextArea fx:id="dateArea" editable="false" layoutX="295.0" layoutY="280.0" prefHeight="25.0" prefWidth="289.0" />
                <Label layoutX="397.0" layoutY="167.0" prefHeight="17.0" prefWidth="85.0" text="obecny czas:" />
                <Button fx:id="closeButton" layoutX="540.0" layoutY="361.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="76.0" text="cancel"  onAction="#closeApp"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
